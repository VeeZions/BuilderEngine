{% macro tags(seo) %}
    {% set locale = app.request.getLocale() %}
    {% set head_office = head_office_data() %}
    <meta name="description" content="{{ seo['desc_' ~ locale] }}">
    <meta name="keywords" content="{{ seo['keywords_' ~ locale]|join(', ') }}">
    <meta name="author" content="{{ head_office.societyName }}">
    <meta name="language" content="{{ app.request.getLocale() }}">
    <meta name="category" content="Software">
    <meta name="copyright" content="{{ head_office.societyName }}">
    <meta name="robots" content="index,follow">
    <meta name="rating" content="general">
    <meta name="twitter:card" content="summary_large_image">
    <meta property="og:title" content="{{ head_office.societyName }} | {{ seo['title_' ~ locale] }}">
    <meta property="og:description" content="{{ seo['desc_' ~ locale] }}">
    <meta property="og:url" content="{{ scheme_and_host }}">
    <meta property="og:type" content="website">
    <meta property="og:locale" content="{{ app.request.getLocale() }}_{{ app.request.getLocale()|upper }}">
    <meta property="og:image" content="{{ asset('media/web/lcb-solutions.jpg') }}">
    <meta property="og:site_name" content="{{ head_office.societyName }}">
{% endmacro %}
